@model PhoneShop.Models.Shipping_Method
@{
    ViewData["Title"] = "Update_ShippingMethod_Seller";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutEmpty.cshtml";
}
@if (TempData["Success"] != null)
{
    <div class="alert alert-success">
        @TempData["Success"]
    </div>
}

<form asp-action="Update_ShippingMethod_Seller" asp-controller="Info_Seller" method="post">

    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <div class="card">
        <div class="card-header">
            <h4 class="card-title">Cập nhật phương thức thanh toán</h4>
        </div>
        <div class="card-body">
            <div class="tab-content">
                <div class="tab-pane active" id="activity">


                   
                    
                    <input asp-for="Id" hidden />
                    <input asp-for="BoothId" hidden />




                    <div class="form-group">
                        <label asp-for="COD" class="control-label">Thanh toán khi nhận hàng</label>
                        <select asp-for="COD" class="form-control">
                            
                            <option value="0">Không</option>
                            <option value="1">Có</option>
                           
                        </select>
                        <span asp-validation-for="COD" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Online_Payment" class="control-label">Thanh toán trực tuyến</label>
                        <select asp-for="Online_Payment" class="form-control">

                            <option value="0">Không</option>
                            <option value="1">Có</option>

                        </select>
                        <span asp-validation-for="Online_Payment" class="text-danger"></span>
                    </div>













                </div>


            </div>

            <!-- /.tab-content -->
        </div><!-- /.card-body -->
        <div class="card-footer">
            <div class="form-group">
                <input class="btn btn-success" type="submit" value="Cập nhật" />
            </div>
        </div>
    </div>

</form>


